{
  "name": "PixelHeart",
  "version": "1.0.0",
  "description": "A basic WebGL app with TypeScript and React",
  "main": "index.js",
  "scripts": {
    "dist:build": "npm run clean && npm run build:sprites && npm run build:shaders && npm run build:maps && tsc --noEmit && npm run build:js && npm run build:static",
    "dist:serve": "npm run dist:build && ts-node ./build/serve.ts",
    "dist:watch": "npm run clean && npm run watch -- --production",
    "build:js": "ts-node ./build/js.ts",
    "build:sprites": "ts-node ./build/sprites.ts",
    "build:shaders": "ts-node ./build/shaders.ts",
    "build:maps": "ts-node ./build/maps.ts",
    "build:static": "ts-node ./build/static.ts",
    "dev": "npm run clean && npm run watch",
    "watch": "ts-node ./build/watch.ts",
    "clean": "rimraf www && mkdir www && rimraf src/shaders game/client/generated"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/code-frame": "^7.22.13",
    "@fsouza/prettierd": "^0.24.2",
    "@parcel/watcher": "^2.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/babel__code-frame": "^7.0.3",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/ws": "^8.5.10",
    "@types/yargs": "^17.0.24",
    "ajv": "^8.12.0",
    "ase-parser": "^0.0.16",
    "autoprefixer": "^10.4.16",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "esbuild": "^0.12.15",
    "express": "^4.18.2",
    "gl-matrix": "^3.4.3",
    "postcss": "^8.4.32",
    "rimraf": "^5.0.1",
    "sharp": "^0.32.4",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5",
    "webpack-glsl-minify": "^1.5.0",
    "ws": "^8.15.0",
    "yargs": "^17.7.2"
  }
}
