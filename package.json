{
  "name": "PixelHeart",
  "version": "1.0.0",
  "description": "A basic WebGL app with TypeScript and React",
  "main": "index.js",
  "scripts": {
    "dist:build": "npm run clean && npm run build:sprites && npm run build:shaders && tsc --noEmit && npm run build:js",
    "dist:serve": "npm run dist:build && ts-node ./build/serve.ts",
    "dist:watch": "npm run clean && npm run watch -- --production",
    "build:js": "ts-node ./build/js.ts",
    "build:sprites": "ts-node ./build/sprites.ts",
    "build:shaders": "ts-node ./build/shaders.ts",
    "dev": "npm run clean && npm run watch",
    "watch": "ts-node ./build/watch.ts",
    "clean": "rimraf www/js && rimraf www/sprites && rimraf src/client/sprites && rimraf src/client/shaders"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/code-frame": "^7.22.13",
    "@fsouza/prettierd": "^0.24.2",
    "@parcel/watcher": "^2.2.0",
    "@types/babel__code-frame": "^7.0.3",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/uuid": "^9.0.2",
    "@types/yargs": "^17.0.24",
    "ase-parser": "^0.0.16",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "esbuild": "^0.12.15",
    "express": "^4.18.2",
    "gl-matrix": "^3.4.3",
    "http-proxy-middleware": "^2.0.6",
    "rimraf": "^5.0.1",
    "sharp": "^0.32.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "webpack-glsl-minify": "^1.5.0",
    "yargs": "^17.7.2"
  }
}
